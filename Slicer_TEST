import Rhino.Geometry as rg


class FacadePanel():
    def __init__(self, geometry, layerHeight):

        self.geometry = geometry #initialize
        self.layerHeight = layerHeight

    def mesurePrintTime(self):
        return 100*self.layerHeight


myPanel = FacadePanel(iBrep1, 2)

oGeometry = myPanel.geometry
oPrintTime = myPanel.mesurePrintTime()
